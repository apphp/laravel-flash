{
    "name": "apphp/laravel-flash",
    "description": "Simple Flash messages for Laravel Framework Applications",
    "keywords": ["apphp", "laravel", "laravel-package", "laravel6", "laravel9", "laravel10", "flash messages"],
    "homepage": "https://apphp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Akopyan",
            "email": "admin@apphp.com",
            "homepage": "https://apphp.com/php-framework",
            "role": "Founder and project lead"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.0|^6.0|^7.0|^8.0|^9.0|^10"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-0": {
            "Apphp\\Flash": "src/"
        },
        "files": [
            "src/Apphp/Flash/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Apphp\\Flash\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Apphp\\Flash\\FlashServiceProvider"
            ],
            "aliases": {
                "Flash": "Apphp\\Flash\\Flash"
            }
        }
    }
}
